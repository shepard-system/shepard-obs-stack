{
  "uid": "shepherd-session-timeline",
  "id": null,
  "title": "Shepherd / 08 Session Timeline",
  "description": "Session Timeline: synthetic OpenTelemetry traces parsed from AI CLI session logs (Claude, Codex, Gemini). Visualizes tool calls, sub-agents, compactions, and MCP operations as a waterfall.",
  "editable": true,
  "refresh": "1m",
  "weekStart": "monday",
  "panels": [
    {
      "id": 100,
      "title": "Overview",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "panels": []
    },
    {
      "id": 1,
      "title": "Sessions",
      "description": "Distinct session traces in the selected time range.",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "round(sum(increase(traces_spanmetrics_calls_total{service=~\"$provider\", span_name=~\"(claude|codex|gemini)\\\\.session\"}[$__range])))",
          "instant": true,
          "legendFormat": "__auto",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "fixedColor": "green",
            "mode": "fixed"
          },
          "unit": "short",
          "decimals": 0,
          "noValue": "0",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 2,
      "title": "Tool Spans",
      "description": "Tool call spans across all sessions.",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 1
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "round(sum(increase(traces_spanmetrics_calls_total{service=~\"$provider\", span_name=~\"(claude|codex|gemini)\\\\.tool\\\\..*\"}[$__range])))",
          "instant": true,
          "legendFormat": "__auto",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "fixedColor": "orange",
            "mode": "fixed"
          },
          "unit": "short",
          "decimals": 0,
          "noValue": "0",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "orange",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 3,
      "title": "Error Spans",
      "description": "Tool spans with error status across all sessions.",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 1
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "round(sum(increase(traces_spanmetrics_calls_total{service=~\"$provider\", span_name=~\"(claude|codex|gemini)\\\\.tool\\\\..*\", status_code=\"STATUS_CODE_ERROR\"}[$__range])))",
          "instant": true,
          "legendFormat": "__auto",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "fixedColor": "red",
            "mode": "fixed"
          },
          "unit": "short",
          "decimals": 0,
          "noValue": "0",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 4,
      "title": "Agent Spans",
      "description": "Sub-agent and compaction spans across all sessions.",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 1
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "round(sum(increase(traces_spanmetrics_calls_total{service=~\"$provider\", span_name=~\"(claude|codex|gemini)\\\\.(agent|compaction)(\\\\..+)?\"}[$__range])))",
          "instant": true,
          "legendFormat": "__auto",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "fixedColor": "purple",
            "mode": "fixed"
          },
          "unit": "short",
          "decimals": 0,
          "noValue": "0",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "purple",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "colorMode": "value",
        "graphMode": "area"
      }
    },
    {
      "id": 200,
      "title": "Trace Search",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 5
      },
      "panels": []
    },
    {
      "id": 5,
      "title": "Session Traces",
      "description": "Search and browse session traces. Click a Trace ID to open the waterfall in Explore.",
      "type": "table",
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 6
      },
      "datasource": {
        "type": "tempo",
        "uid": "tempo"
      },
      "targets": [
        {
          "datasource": {
            "type": "tempo",
            "uid": "tempo"
          },
          "queryType": "traceql",
          "query": "{ resource.service.name =~ \"$provider\" }",
          "tableType": "traces",
          "limit": 20,
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            }
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Trace ID"
            },
            "properties": [
              {
                "id": "links",
                "value": [
                  {
                    "title": "Open trace",
                    "url": "/explore?schemaVersion=1&panes=%7B%22one%22:%7B%22datasource%22:%22tempo%22,%22queries%22:%5B%7B%22refId%22:%22A%22,%22datasource%22:%7B%22type%22:%22tempo%22,%22uid%22:%22tempo%22%7D,%22queryType%22:%22traceql%22,%22query%22:%22${__value.raw}%22%7D%5D%7D%7D"
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Duration"
            },
            "properties": [
              {
                "id": "unit",
                "value": "ns"
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Start time",
            "desc": true
          }
        ],
        "cellHeight": "sm"
      }
    },
    {
      "id": 300,
      "title": "Tool Call Analysis",
      "type": "row",
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 18
      },
      "panels": []
    },
    {
      "id": 6,
      "title": "Top Tools by Invocation Count",
      "description": "Tools ranked by number of invocations across all session traces.",
      "type": "table",
      "gridPos": {
        "h": 10,
        "w": 12,
        "x": 0,
        "y": 19
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "round(sum by (span_name) (increase(traces_spanmetrics_calls_total{service=~\"$provider\", span_name=~\"(claude|codex|gemini)\\\\.tool\\\\..*\"}[$__range]))) > 0",
          "instant": true,
          "format": "table",
          "legendFormat": "__auto",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "red",
                "value": 200
              }
            ]
          },
          "color": {
            "mode": "thresholds"
          }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Invocations",
            "desc": true
          }
        ],
        "cellHeight": "sm"
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "renameByName": {
              "span_name": "Tool",
              "Value": "Invocations"
            }
          }
        },
        {
          "id": "sortBy",
          "options": {
            "sort": [
              {
                "field": "Invocations",
                "desc": true
              }
            ]
          }
        }
      ]
    },
    {
      "id": 7,
      "title": "Tool Duration Distribution (p95)",
      "description": "P95 tool span durations over time by tool name.",
      "type": "timeseries",
      "gridPos": {
        "h": 10,
        "w": 12,
        "x": 12,
        "y": 19
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "histogram_quantile(0.95, sum by (le, span_name) (rate(traces_spanmetrics_latency_bucket{service=~\"$provider\", span_name=~\"(claude|codex|gemini)\\\\.tool\\\\..*\"}[$__rate_interval])))",
          "legendFormat": "{{span_name}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            }
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "mean",
            "max"
          ]
        }
      }
    },
    {
      "id": 8,
      "title": "Error Tools",
      "description": "Tool spans that completed with error status.",
      "type": "table",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 29
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "round(sum by (span_name, service) (increase(traces_spanmetrics_calls_total{service=~\"$provider\", span_name=~\"(claude|codex|gemini)\\\\.tool\\\\..*\", status_code=\"STATUS_CODE_ERROR\"}[$__range]))) > 0",
          "instant": true,
          "format": "table",
          "legendFormat": "__auto",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "align": "auto",
            "cellOptions": {
              "type": "auto"
            }
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              }
            ]
          },
          "color": {
            "mode": "thresholds"
          }
        },
        "overrides": []
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Errors",
            "desc": true
          }
        ],
        "cellHeight": "sm"
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "renameByName": {
              "span_name": "Tool",
              "service": "Provider",
              "Value": "Errors"
            }
          }
        },
        {
          "id": "sortBy",
          "options": {
            "sort": [
              {
                "field": "Errors",
                "desc": true
              }
            ]
          }
        }
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "name": "provider",
        "label": "Provider",
        "type": "custom",
        "query": "claude-code-session,codex-session,gemini-session",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "options": [
          {
            "selected": true,
            "text": "All",
            "value": "$__all"
          },
          {
            "selected": false,
            "text": "Claude",
            "value": "claude-code-session"
          },
          {
            "selected": false,
            "text": "Codex",
            "value": "codex-session"
          },
          {
            "selected": false,
            "text": "Gemini",
            "value": "gemini-session"
          }
        ],
        "includeAll": true,
        "allValue": ".*-session",
        "multi": false
      }
    ]
  },
  "tags": [
    "session-timeline",
    "traces",
    "shepherd"
  ],
  "timezone": "browser",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "version": 2,
  "schemaVersion": 39,
  "graphTooltip": 1,
  "links": [],
  "timepicker": {},
  "annotations": {
    "list": []
  }
}
